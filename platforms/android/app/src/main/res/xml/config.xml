<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="41000" id="com.aviva.travel" version="4.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <name>Aviva Travel</name>
    <description>
        Aviva Singapore Travel 
    </description>
    <author email="tao_liu@aviva-asia.com" href="">
    </author>
    <content src="https://login.aviva.com.sg/gi" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://login.aviva.com.sg/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="res/icons/android/icon-ldpi.png" />
    <icon density="mdpi" src="res/icons/android/icon-mdpi.png" />
    <icon density="hdpi" src="res/icons/android/icon-hdpi.png" />
    <icon density="xhdpi" src="res/icons/android/icon-xhdpi.png" />
    <icon density="xxhdpi" src="res/icons/android/icon-xxhdpi.png" />
    <icon density="xxxhdpi" src="res/icons/android/icon-xxxhdpi.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
