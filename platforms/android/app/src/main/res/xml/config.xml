<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="42000" id="com.aviva.travel" version="4.3.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <name>Singlife Travel</name>
    <description>
        Singlife Travel 
    </description>
    <author email="tao_liu@aviva-asia.com" href="">
    </author>
    <content src="https://login.aviva.com.sg/gi" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://login.aviva.com.sg/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon src="res/icons/android/icon.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
